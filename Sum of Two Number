// Take Two Input from the User.
import java.util.Scanner;
class Test1{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the First Number:");
        int a=sc.nextInt();

        System.out.println("Enter the First Number:");
        int b=sc.nextInt();

        int sum=a+b;
        System.out.println("Result:"+sum);
    }
}
